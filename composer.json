{
    "name": "stephane888/drupal_project_habeuk",
    "description": " Project template drupal for developpeur habeuk.com ",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Habeuk",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal-composer/drupal-project": "9.x-dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
    },
    "scripts": {
    },
    "extra": {
        "installer-paths": {
            "web/modules/custom/{$name}": ["type:drupal-module-custom"],
            "web/themes/custom/{$name}": ["type:drupal-theme-custom"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
        },
        "patches": {
        }
    }
}
